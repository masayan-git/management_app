<p id="notice"><%= notice %></p>

<h1>Residences Residence Messages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @residences_residence_messages.each do |residences_residence_message| %>
      <tr>
        <td><%= link_to 'Show', residences_residence_message %></td>
        <td><%= link_to 'Edit', edit_residences_residence_message_path(residences_residence_message) %></td>
        <td><%= link_to 'Destroy', residences_residence_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

