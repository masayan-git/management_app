<p id="notice"><%= notice %></p>

<h1>Apartments Articles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3">Title</th>
    </tr>
  </thead>

  <tbody>
    <% @apartments_articles.each do |article| %>
      <tr>
        <td><%= article.title%></td>
        <td><%= link_to 'Show', apartments_article_path(article.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>



